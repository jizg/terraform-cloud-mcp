# Docker Compose Environment Variables
# Copy this file to .env and update with your actual values

# Terraform Cloud API Token (required)
TFC_TOKEN=your-terraform-cloud-token-here

# Terraform Cloud Address (optional)
# TFC_ADDRESS=https://app.terraform.io

# Safety Controls (optional)
# ENABLE_DELETE_TOOLS=false
# READ_ONLY_TOOLS=false

# Response Filtering (optional)
# ENABLE_RAW_RESPONSE=false
